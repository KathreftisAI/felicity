    function CreateWorkorderTable(WorkOrderBy){
        $.ajax({ 
            url: Core.Config.Get('Baselink') + 'Action=GetChangeWorkorderDetails;',
            type: "GET",
            data: {ChangeID : change_id,OrderBy:WorkOrderBy},
            success : function(data){
                data = $.parseJSON(data);
                var str = '';
