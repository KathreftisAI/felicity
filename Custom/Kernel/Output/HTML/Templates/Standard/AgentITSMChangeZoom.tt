                        var wo_no = '[% Data.ChangeNumber %]' +" - "+ v.WorkOrderNumber;
                        str += "\
                        <tr class='Last'>\
                            <td class=''><span >" + v.WorkOrderAgent+ "</span></td>\
                            <td class=''>" + wo_no +"</td>\
                            <td class=''><span >" + v.WorkOrderTitle+"</span></td>\
                            <td class=''><span >" + v.WorkOrderType+"</span></td>\
                            <td class=''>" + v.WorkOrderState+ "</td>\
                            <td class=''>" +v.CreateTime + "</td>\
                            <td class=''><span >" + v.PlannedStartTime+ "</span></td>\
                            <td class='Last'><span>" +v.CreateBy + "</span></td>\
                        </tr>";
                    });
                    $('#uno_linked_wo').html(str);
                      
                }else{
                    $('#uno_wo_div').hide();  
                }
                
            }
        }); 
