<script type="text/javascript">//<![CDATA[
    $('ul.Actions a.AsPopup').bind('click', function (Event) {
    Core.UI.Popup.OpenPopup ($(this).attr('href'), 'Action');
    return false;
    });

    $('ul.Actions a.HistoryBack').bind('click', function (Event) {
        history.back();
        return false;
    });

    var sessionid ="[% Env("SessionID") %]";
    var change_id ="[%  Data.ChangeID %]";
    var TicketInfo=[% JSON(Data.Ticket) %];
    var LinkedTicketCount=$('.DataTable tr a').size();

    //If Linked ticket is there than add show ticket detail
    for (var i = 0; i < LinkedTicketCount; i++) {
        if (i==0) {
            //added table head show details
             $('.DataTable tr').eq(i).append("<th>Show Details</td>");
        }
        //get TicketNumber
        var LinkedTicketNumber=parseInt($(".DataTable").find("a").eq(i).html());
        var IncrementedI=i+1;
        //add show more details td
        $('.DataTable tr').eq(IncrementedI).append("<td><span class='ticketDetails' id="+LinkedTicketNumber+"> Show Details </span></td>");
    }
    //on click span show details call GetTicketDetails function 
    $('.DataTable span.ticketDetails').on('click',function(){GetTicketDetails( $(this).attr('id'));});



#Fetch the ticket details based on show details click event*
	function GetTicketDetails(ticketno){
		var ticket_no = ticketno;
        $.ajax({
    		url: Core.Config.Get('Baselink') + 'Action=GetTicketDetails',
    		type: "GET",
    		data: {TicketNumber : ticket_no},
    		success : function(data){
    			var TicketNumberInfo = $.parseJSON(data);

