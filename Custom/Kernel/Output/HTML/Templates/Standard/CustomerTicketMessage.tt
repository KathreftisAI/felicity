                <div id="BottomActionRow">
                    <button class="btn btn-sm btn-primary" id="submitRichText" accesskey="g" title="[% Translate("Submit") | html %] (g)" type="submit" value="[% Translate("Submit") | html %]">[% Translate("Submit") | html %]</button>
                </div>
            </form>
        </div>
    </div>
