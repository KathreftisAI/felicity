                    <fieldset class="TableLike">
                        <label>[% Translate("Records") | html %]:</label>
                        <div class="Value">[% Data.Counter | html %]</div>
                        <div class="Clear"></div>

                        <label>[% Translate("Success") | html %]:</label>
