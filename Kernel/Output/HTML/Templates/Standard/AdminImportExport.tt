[% RenderBlockEnd("TemplateEdit4DownButton") %]
[% RenderBlockStart("TemplateEdit4NoDownButton") %]
                                    <button class="ArrowDown" type="submit" disabled="disabled"> [% Translate("Down") | html %]</button>
[% RenderBlockEnd("TemplateEdit4NoDownButton") %]
