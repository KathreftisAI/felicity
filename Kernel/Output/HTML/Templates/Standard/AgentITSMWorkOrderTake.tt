<fieldset class="TableLike W300px">
    <label>[% Translate("Title") | html %]:</label>
    <p class="Value">
        [% Data.WorkOrderTitle | html %]
    </p>
    <div class="Clear"></div>

    <label>[% Translate("Current Agent") | html %]:</label>
    <p class="Value">
        [% Data.WorkOrderAgent | html %]
    </p>
    <div class="Clear"></div>

</fieldset>
<p class="SpacingTop Center">
    [% Translate("Do you really want to take this workorder?") | html %]
</p>
