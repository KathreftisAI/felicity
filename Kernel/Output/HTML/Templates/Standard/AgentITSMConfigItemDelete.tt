<fieldset class="TableLike W300px">

    <label>[% Translate("Name") | html %]:</label>
    <p class="Value">
        [% Data.Name | html %]
    </p>
    <div class="Clear"></div>

    <label>[% Translate("Number") | html %]:</label>
    <p class="Value">
        [% Translate(Data.Number) | html %]
    </p>
